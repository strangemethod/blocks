<section class="block hero" data-block="{{id}}">
  {{> image-block/image-block
			blockId=id
			blockType="hero"
			fieldId="image"
			fieldType="image"
			imageSrc=image
	}}
	<div class="hero-overlay">
		{{> text-block/text-block
  			blockId=id
  			blockType="hero"
				element="h1"
  			fieldId="text"
  			fieldType="hero"
				imageSrc=image
  			text=title
  	}}
	</div>
</section>