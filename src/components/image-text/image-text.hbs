<section class="block text-image container">
  <div class="grid">
    <div class="grid-col grid-col--5 text-image__text">
      {{> text-block/text-block
          blockId=id
          blockType="text-image"
          element="p"
          fieldId="text"
          fieldType="text"
          text=text
      }}
    </div>
    <div class="grid-col grid-col--1"></div>
    <div class="grid-col grid-col--6">
      {{> image-block/image-block
          blockId=id
          blockType="text-image"
          fieldId="image"
          fieldType="image"
          imageSrc=image
        }}
    </div>
  </div>
</section>