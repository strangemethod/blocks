<section class="block centered-text container">
	<div class="grid">
		<div class="grid-col grid-col--2"></div>
		<div class="grid-col grid-col--8">
			{{> text-block/text-block
	  			blockId=id
	  			blockType="text-image"
					element="h2"
	  			fieldId="text"
	  			fieldType="text"
	  			text=text
	  	}}
		</div>
		<div class="grid-col grid-col--2"></div>
	</div>
</section>